  section#fh5co-counters(style='background-image: url(images/full_image_1.jpg);', data-stellar-background-ratio='0.5')
    .fh5co-overlay
    .container
      .row
        .col-md-12.section-heading.text-center.to-animate
          h2 Fun Facts
      .row
        .col-md-3.col-sm-6.col-xs-12
          .fh5co-counter.to-animate
            i.fh5co-counter-icon.icon-briefcase.to-animate-2
            span.fh5co-counter-number.js-counter(data-from='0', data-to='25', data-speed='5000', data-refresh-interval='50') 25
            span.fh5co-counter-label Finished projects
        .col-md-3.col-sm-6.col-xs-12
          .fh5co-counter.to-animate
            i.fh5co-counter-icon.icon-leaf.to-animate-2
            span.fh5co-counter-number.js-counter(data-from='0', data-to='2688', data-speed='5000', data-refresh-interval='50') 2688
            span.fh5co-counter-label Plants planted
        .col-md-3.col-sm-6.col-xs-12
          .fh5co-counter.to-animate
            i.fh5co-counter-icon.icon-home.to-animate-2
            span.fh5co-counter-number.js-counter(data-from='0', data-to='268', data-speed='5000', data-refresh-interval='50') 268
            span.fh5co-counter-label Happy families
        .col-md-3.col-sm-6.col-xs-12
          .fh5co-counter.to-animate
            i.fh5co-counter-icon.icon-emotsmile.to-animate-2
            span.fh5co-counter-number.js-counter(data-from='0', data-to='1088', data-speed='5000', data-refresh-interval='50') 1088
            span.fh5co-counter-label People helped
  section#fh5co-contact(data-section='contact')
    .container
      .row
        .col-md-12.section-heading.text-center
          h2.to-animate Get In Touch
          .row
            .col-md-8.col-md-offset-2.subtext.to-animate
              h3
                | Far far away, behind the word mountains, far from the countries Vokalia and Consonantia, there live the blind texts.
      .row.row-bottom-padded-md
        .col-md-6.to-animate
          h3 Contact Info
          ul.fh5co-contact-info
            li.fh5co-contact-address
              i.icon-home
              | Grha XL
            li
              i.icon-phone
              |  (123) 465-6789
            li
              i.icon-envelope
              | info@nannypot.co
            li
              i.icon-globe
              a(href='#', target='_blank') Nanny Pot
        .col-md-6.to-animate
          h3 Contact Form
          .form-group
            label.sr-only(for='name') Name
            input#name.form-control(placeholder='Name', type='text')
          .form-group
            label.sr-only(for='email') Email
            input#email.form-control(placeholder='Email', type='email')
          .form-group
            label.sr-only(for='phone') Phone
            input#phone.form-control(placeholder='Phone', type='text')
          .form-group
            label.sr-only(for='message') Message
            textarea#message.form-control(name='', cols='30', rows='5', placeholder='Message', style='resize:none;')
          .form-group
            input.btn.btn-primary.btn-lg(value='Send Message', type='submit')
    #map.to-animate
  footer#footer(role='contentinfo')
    a.gotop.js-gotop(href='#')
      i.icon-arrow-up2
    .container
      div
        .col-md-12.text-center
          p
            | © #{title}. All Rights Reserved. 
      .row
        .col-md-12.text-center
          ul.social.social-circle
            li
              a(href='#')
                i.icon-twitter
            li
              a(href='#')
                i.icon-facebook
            li
              a(href='#')
                i.icon-youtube